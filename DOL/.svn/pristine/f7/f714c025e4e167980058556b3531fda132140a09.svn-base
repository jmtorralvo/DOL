<body ng-app="endesaApp" ng-controller="AppCtrl">

    <!-- Logo and Navigation -->
    <header class="site-header">

        <div class="container">
            <div class="row">
                <div class="col-xs-3 col-sm-2">
                    <section class="site-logo">
                        <a href="" ng-click="irAMisDiagnosticos()">
                            <img src="dol/static/images/logo-endesa.png">
                        </a>
                    </section>
                </div>
                <div class="col-xs-9 col-sm-4">
                    <h1>Diagnóstico energético
                        <small>Online</small>
                    </h1>
                </div>

                <div class="col-xs-offset-3 col-xs-9 col-sm-offset-0 col-sm-6">
                    <div class="row">
                        <div class="col-xs-4 col-sm-2 col-sm-offset-0 col-md-1 col-md-offset-4 col-lg-1 col-lg-offset-4">
                            <div class="pull-left hidden-sm hidden-md hidden-lg">
                                <ol class="breadcrumb">
                                    <li ng-show="user.userInfo.mail!==undefined">
                                        <a href="" ng-click="irAMisDiagnosticos()" class="enlace_home">
                                            <span class="glyphicon glyphicon-home"></span>Mis diagnósticos</a>
                                    </li>
                                </ol>
                            </div>
                        </div>
                        <div class="col-xs-3 col-sm-4 col-md-3 col-lg-3">
                            <div class="pull-right select_language mt17-xs dropdown header-link-language" ng-show="esFormulario() === false">
                                <a ng-href="#" class="dropdown-toggle">
                                    <span class="language">{{idioma.descripcion}}</span>
                                    <span class="hidden-xs text-hide dropdown-icon"></span>
                                </a>
                                <ul class="dropdown-menu" >
                                    <li ng-repeat="idioma in idiomas">
                                        <a ng-click = "cambiarDeIdioma(idioma)" ng-href="" >
                                           {{idioma.descripcion}}
                                        </a>
                                    </li>
                                </ul>
                            </div>
                        </div>
                        <div class="col-xs-5 col-sm-6 col-md-4 col-lg-4">
                            <div class="pull-right" ng-show="user.userInfo.mail!==undefined">
                                <ul class="list-inline pull-right header-links">
                                    <li class="profile-info mr30-xs dropdown">
                                        <a ng-href="#" class="dropdown-toggle">
                                            <img src="dol/static/images/user-profile-image.png" alt="" class="img-circle" width="44">
                                            <span class="hidden-xs text-hide dropdown-icon"></span>
                                        </a>
                                        <ul class="dropdown-menu profile">
                                            <!-- Reverse Caret -->
                                            <!-- <li class="caret"></li> -->
                                            <!-- Profile sub-links -->
                                            <li>
                                                <a href="" ng-click="irAMisDiagnosticos()">
                                                    {{user.userInfo.mail}}
                                                </a>
                                            </li>
                                            <li ng-show="user.userInfo.perfilUsuario == 'USR-01'">
                                                <a ui-sref="cambio-contraseña">
                                                    Cambiar contraseña
                                                </a>
                                            </li>                                           
                                            <li>
                                                <a target="_blank" ui-sref="terminos-condiciones">
                                                    Términos y Condiciones
                                                </a>
                                            </li>
                                            <li ng-show="user.userInfo.perfilUsuario == 'USR-01'">
                                                <a href="" ng-click="openEliminarCuenta()">
                                                    Eliminar cuenta
                                                </a>
                                            </li>
                                        </ul>
                                    </li>
                                    <li>
                                        <a href="" ng-click="realizarLogout()" class="logout text-hide">Log Out</a>
                                    </li>
                                </ul>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
        </div>
    </header>
    <section class="breadcrumb-container hidden-xs">
        <div class="container">
            <div class="col-sm-offset-2">
                <ol class="breadcrumb">
                    <li ng-show="user.userInfo.mail!==undefined">
                        <a href="" ng-click="irAMisDiagnosticos()">
                            <span class="glyphicon glyphicon-home"></span>Mis diagnósticos</a>
                    </li>
                </ol>
            </div>
        </div>
    </section>



    <!-- Site main content -->
    <section class="main-content">
        <div ui-view></div>
    </section>

    <!-- Site Footer -->
    <footer class="site-footer">
        <div class="container">
            <div class="row">
                <div class="col-sm-4 col-xs-6">
                    <p>Endesa S.A.
                        <strong>2014</strong>
                    </p>
                </div>
                <div class="col-sm-8 col-xs-6">
                    <ul class="subpienel">
                        <li class="hidden-xs">
                            <a target="_blank" ui-sref="aviso-legal" class="text-info" title="Aviso Legal">Aviso Legal</a>
                        </li>

                        <li>
                            <a target="_blank" href="http://www.enel.com/" target="_blank" class="text-hide grupoEnel" title="Grupo Enel">Grupo Enel</a>
                        </li>
                    </ul>
                </div>
            </div>
        </div>
    </footer>

    <div ng-if="cookies.aceptadas !== true" class="cookie-warning">
        <span class="aviso">Esta página web utiliza cookies para mejorar la experiencia de usuario. Utilizando nuestra web consientes la utilización de cookies de acuerdo a nuestra <a href="http://www.endesa.com/es/CookiesEndesa" target="_blank">Política de Cookies</a>
        </span>
        <div class="btn-group">
            <button type="button" ng-click="aceptarCookies()" class="btn btn-guardar">Acepto</button>
        </div>
    </div>

